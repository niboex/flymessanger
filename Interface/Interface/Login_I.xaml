<Window x:Class="Interface.Login_I"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Interface"
        mc:Ignorable="d"
        Title="Login_I" Height="300" Width="300" WindowStyle="None">
    <Grid>
        <TextBox x:Name="Login" HorizontalAlignment="Left" Height="23" Margin="86,82,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="120" TextChanged="Login_TextChanged"/>
        <TextBox x:Name="Pass" HorizontalAlignment="Left" Height="23" Margin="86,126,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="120"/>
        <Button x:Name="button" Content="Button" HorizontalAlignment="Left" Margin="107,178,0,0" VerticalAlignment="Top" Width="75" Click="button_Click" IsCancel="True"/>
        <Button x:Name="button1" Content="Съебаться" HorizontalAlignment="Left" Margin="188,0,0,0" VerticalAlignment="Top" Width="104" Height="32" Click="button1_Click"/>

    </Grid>
</Window>
